{
    "name": "viviniko/catalog",
    "description": "VIVINIKO Catalog Service Provider",
	"keywords": ["viviniko", "catalog", "laravel"],
	"homepage": "http://viviniko.com",
	"license": "MIT",
	"authors": [
        {
            "name": "viviniko",
            "email": "viviniko.shop@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "viviniko/configuration": "^1.0",
        "viviniko/favorite": "^1.0",
        "viviniko/media": "^1.0",
        "viviniko/review": "^1.0",
        "viviniko/repository": "^1.0",
        "viviniko/scoutelasticsearch": "^1.0",
        "viviniko/tag": "^1.0",
        "viviniko/support": "^1.0",
        "viviniko/urlrewrite": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "Viviniko\\Catalog": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viviniko\\Catalog\\CatalogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
