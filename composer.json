{
    "name": "viviniko/catalog",
    "description": "VIVINIKO Catalog Service Provider",
	"keywords": ["viviniko", "catalog", "laravel"],
	"homepage": "http://viviniko.com",
	"license": "MIT",
	"authors": [
        {
            "name": "viviniko",
            "email": "viviniko.shop@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "viviniko/cart": "dev-master",
        "viviniko/configuration": "dev-master",
        "viviniko/currency": "dev-master",
        "viviniko/favorite": "dev-master",
        "viviniko/media": "dev-master",
        "viviniko/rewrite": "dev-master",
        "viviniko/review": "dev-master",
        "viviniko/repository": "dev-master",
        "viviniko/scoutelasticsearch": "dev-master",
        "viviniko/tag": "dev-master",
        "viviniko/support": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Viviniko\\Catalog": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Viviniko\\Catalog\\CatalogServiceProvider"
            ],
            "aliases": {
                "Attrs": "Viviniko\\Catalog\\Facades\\Attrs",
                "AttrValues": "Viviniko\\Catalog\\Facades\\AttrValues",
                "ProductSpecs": "Viviniko\\Catalog\\Facades\\ProductSpecs",
                "ProductSpecValues": "Viviniko\\Catalog\\Facades\\ProductSpecValues"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
